<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<navigation-rule>
		<from-view-id>/pages/eyetracker/viewIssues.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/pages/eyetracker/viewIssue.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>createIssue</from-outcome>
			<to-view-id>/pages/eyetracker/createIssue.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/eyetracker/viewIssue.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>backToList</from-outcome>
			<to-view-id>/pages/eyetracker/viewIssues.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>entryAdded</from-outcome>
			<to-view-id>/pages/eyetracker/viewIssue.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/pages/eyetracker/TODO.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
